    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "zx": "^8.4.0"
  },
  "devDependencies": {
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "debug": "^4.4.0",
    "is-ci": "^4.1.0",
    "plop": "^4.0.1",
    "read-package-up": "^11.0.0",
    "supports-color": "^10.0.0"
  },
  "peerDependencies": {
    "@biomejs/biome": "1.9.4"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  }
}
