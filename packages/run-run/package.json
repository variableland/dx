    "@biomejs/biome": "1.9.4",
    "rimraf": "6.0.1",
    "typescript": "5.8.2",
    "zx": "8.4.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "bun": ">=1.0.0"
